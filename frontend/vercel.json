{
  "routes": [
    {
      "src": "/auth/callback",
      "dest": "/"
    },
    {
      "src": "/auth/v1/callback",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 